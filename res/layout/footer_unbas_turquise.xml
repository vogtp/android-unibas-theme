	android:orientation="horizontal" android:layout_width="fill_parent">
	<ImageView android:layout_weight=".5" android:layout_gravity="left|bottom"
		android:id="@+id/imageView1" android:src="@drawable/unituerkis"
		android:minHeight="160dp" android:layout_width="wrap_content"
