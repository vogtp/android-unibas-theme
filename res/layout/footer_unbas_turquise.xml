<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout1" android:layout_height="wrap_content"
	android:orientation="horizontal" android:minHeight="150dp" android:layout_width="fill_parent">
	<ImageView android:layout_weight=".2" android:layout_gravity="left|bottom"
		android:id="@+id/imageView1" android:src="@drawable/unituerkis"
		android:minHeight="150dp" android:maxHeight="150dp" android:layout_width="wrap_content"
		android:layout_height="fill_parent" android:adjustViewBounds="true"
		></ImageView>
	<TextView android:id="@+id/textView1" android:layout_weight="2.7"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></TextView>
</LinearLayout>

